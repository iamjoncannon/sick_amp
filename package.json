{
  "name": "sickamp",
  "version": "1.0.0",
  "main": "main.js",
  "scripts": {
    "test-summary": "jest --config=jest.config.js --coverage --watch -- -u",
    "test": "jest $1 --config=jest.config.js --watch -- -u",
    "sass": "node-sass scss/main.scss public/style.css -w -r",
    "sass-bund": "node-sass scss/main.scss public/style.css",
    "sh": "cd ~/.ssh & ssh -i $key ubuntu@$gopher_api_addy",
    "dep-install": "chmod +x ./deploy/install && ./deploy/install ubuntu@$addy",
    "dep-nginx": "chmod +x ./deploy/nginxConfig && ./deploy/nginxConfig",
    "deploy": "addy=$addy key=$key pm2 deploy ecosystem.config.js production",
    "start-dev": "webpack -w --progress",
    "start-server": "nodemon -L --watch main.js -e html,js,css",
    "build": "webpack -w --progress",
    "start": "echo 'Running production server' && node server.js",
    "site": "open http://$addy",
    "compile": "NODE_ENV=production webpack -p --progress",
    "install-lazy": "npm install webpack@4.28.4 && npm install acorn-dynamic-import@4.0.0 && npm update acorn --depth 20 && npm dedupe"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@material-ui/icons": "^4.5.1",
    "@types/express": "^4.17.1",
    "@types/node": "^12.11.7",
    "@types/react": "^16.9.9",
    "@types/styled-components": "^4.1.8",
    "acorn-dynamic-import": "^4.0.0",
    "axios": "^0.18.0",
    "babel-polyfill": "^6.26.0",
    "body-parser": "^1.18.2",
    "cross-fetch": "^3.0.4",
    "express": "^4.14.0",
    "moment": "^2.24.0",
    "moment-duration-format": "^2.2.2",
    "namor": "^1.1.3",
    "node-id3": "^0.1.11",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-redux": "^7.1.1",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.0.1",
    "react-table": "next",
    "redux": "^4.0.4",
    "socket-io": "^1.0.0",
    "socket.io": "^2.3.0",
    "socket.io-client": "^2.3.0",
    "socket.io-stream": "^0.9.1",
    "styled-components": "^4.4.0"
  },
  "devDependencies": {
    "@babel/core": "^7.6.2",
    "@babel/preset-env": "^7.6.2",
    "@babel/preset-react": "^7.0.0",
    "babel-loader": "^8.0.6",
    "css-loader": "^0.28.11",
    "jest": "^24.9.0",
    "style-loader": "^0.21.0",
    "ts-loader": "^6.2.0",
    "typescript": "^3.6.4",
    "webpack": "^4.28.4",
    "webpack-cli": "^3.2.3",
    "@rescripts/cli": "^0.0.11",
    "@rescripts/rescript-use-babel-config": "^0.0.8",
    "@rescripts/rescript-use-eslint-config": "^0.0.9"
  },
  "description": ""
}
