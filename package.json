{
  "name": "sickamp",
  "version": "1.0.0",
  "main": "main.js",
  "scripts": {
    "test-summary": "jest --config=jest.config.js --coverage --watch -- -u",
    "test": "jest $1 --config=jest.config.js --watch -- -u",
    "sass": "node-sass scss/main.scss public/style.css -w -r",
    "sass-bund": "node-sass scss/main.scss public/style.css",
    "sh": "cd ~/.ssh & ssh -i $key ubuntu@$gopher_api_addy",
    "dep-install": "chmod +x ./deploy/install && ./deploy/install ubuntu@$addy",
    "dep-nginx": "chmod +x ./deploy/nginxConfig && ./deploy/nginxConfig",
    "deploy": "addy=$addy key=$key pm2 deploy ecosystem.config.js production",
    "start-dev": "webpack -w --progress",
    "start-server": "nodemon -L --watch main.js -e html,js,css",
    "build": "webpack -w --progress",
    "start": "echo 'Running production server' && node server.js",
    "site": "open http://$addy",
    "compile": "NODE_ENV=production webpack -p --progress",
    "install-lazy": "npm install webpack@4.28.4 && npm install acorn-dynamic-import@4.0.0 && npm update acorn --depth 20 && npm dedupe"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
